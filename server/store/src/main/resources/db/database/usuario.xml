<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet id="USUARIOS" author="VITOR.MANOEL">
        <validCheckSum>ANY</validCheckSum>
        <preConditions onFail="MARK_RAN" onFailMessage="Usuarios jÃ¡ existe no Banco de Dados">
            <not>
                <tableExists tableName="USUARIOS"/>
            </not>
        </preConditions>
        <createTable tableName="USUARIOS">
            <column name="ID" type="bigserial" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="NOME" type="varchar(100)">
                <constraints nullable="false"/>
            </column>
            <column name="EMAIL" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="SENHA" type="varchar(30)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>